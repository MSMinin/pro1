<%console.log("modifyForm list : ", list) %>
<form action="/member/modify" method="post">
    <table>
        <tr>
            <td>아이디</td><td><input type="text" readonly name="id" value="<%=list.ID%>"></td>
        </tr>
        <tr>
            <td>비밀번호</td><td><input type="password" name="pwd" value="<%=list.PWD%>"></td>
        </tr>
        <tr>
            <td>이름</td><td><input type="text" name="name" value="<%=list.NAME%>"></td>
        </tr>
        <tr>
            <td>주소</td><td><input type="text" name="addr" value="<%=list.ADDR%>"></td>
        </tr>
        <tr>
            <td>전화번호</td><td><input type="text" name="pNumber" placeholder="010-0000-0000"
                    required pattern="[0][1][0]-\d{4}-\d{4}" value="<%=list.P_NUMBER%>"></td>
        </tr>
        <tr>
            <td>생년월일</td><td><input type="date" name="birth" value="<%=list.BIRTH%>"></td>
        </tr>
        <tr>
            <td><input type="submit" value="수정하기"></td>
        </tr>
    </table>
</form>